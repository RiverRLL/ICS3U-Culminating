import javax.swing.*;

public class Sell {

    public void sell(){

//List each stock owned and have button to increase the quanity sold of each



    JFrame f=new JFrame("StockSim");  
    JButton buy=new JButton("SELL");  
    buy.setBounds(135,150,120,30);  
    f.add(buy);  
    f.setSize(400,400);  
    f.setLayout(null);  
    f.setVisible(true); 

    JFrame f=new JFrame("StockSim");  
    JButton buy=new JButton("BACK");  
    buy.setBounds(135,200,120,30);  
    f.add(buy);  
    f.setSize(400,400);  
    f.setLayout(null);  
    f.setVisible(true); 


}
}